{
  "name": "spotware-protobuf",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "tsc",
    "build": "tsc",
    "preimport": "shx rm -rf ./assets/*.proto",
    "import": "shx mkdir ./spotware-messages && cd ./spotware-messages && isogit clone --url=https://github.com/spotware/Open-API-2.0-protobuf-messages --singleBranch",
    "postimport": "shx cp ./spotware-messages/*.proto ./assets && shx rm -rf ./spotware-messages",
    "start": "ts-node src/hello-world.ts ./assets/*.proto",
    "poststart": "prettier --write ./assets/**.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/globby": "^9.1.0",
    "@types/node": "^11.13.6",
    "command-line-args": "^5.1.1",
    "globby": "^9.2.0",
    "husky": "^1.3.1",
    "isomorphic-git": "^0.54.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "shx": "^0.3.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  }
