{
  "name": "@claasahl/spotware-connect-api",
  "version": "0.1.1",
  "description": "Protocol Buffers and Data Models for Spotware Open API 2.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepare": "npm run build",
    "prebuild": "shx rm -rf ./build",
    "build": "npm run import && npm run generate && tsc",
    "preimport": "shx rm -rf ./assets/*.proto",
    "import": "shx mkdir ./spotware-messages && cd ./spotware-messages && isogit clone --url=https://github.com/spotware/Open-API-2.0-protobuf-messages --singleBranch",
    "postimport": "shx cp ./spotware-messages/*.proto ./assets && shx rm -rf ./spotware-messages",
    "pregenerate": "shx mkdir ./build",
    "generate": "npm run generate:js && npm run generate:ts && npm run generate:event-emitter && npm run generate:client",
    "generate:js": "pbjs --target static-module --wrap es6 --es6 --out ./src/spotware-messages.js ./assets/*.proto",
    "postgenerate:js": "prettier --write ./src/spotware-messages.js && shx cp ./src/spotware-messages.js ./build/",
    "generate:ts": "pbts --out ./src/spotware-messages.d.ts ./src/spotware-messages.js",
    "postgenerate:ts": "prettier --write ./src/spotware-messages.d.ts && shx cp ./src/spotware-messages.d.ts ./build/",
    "generate:event-emitter": "ts-node ./scripts/spotware-event-emitter.ts",
    "postgenerate:event-emitter": "prettier --write ./src/spotware-event-emitter.ts",
    "generate:client": "ts-node ./scripts/spotware-client.ts",
    "postgenerate:client": "prettier --write ./src/spotware-client.ts"
  },
  "keywords": [
    "spotware",
    "ctrader",
    "typescript",
    "ts",
    "api",
    "protobuf"
  ],
  "author": {
    "name": "Claas Ahlrichs",
    "email": "claasahl@gmail.com",
    "url": "https://github.com/claasahl"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/node": "^11.12.2",
    "@types/protocol-buffers-schema": "^3.3.0",
    "husky": "^1.3.1",
    "isomorphic-git": "^0.53.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "protocol-buffers-schema": "^3.3.2",
    "shx": "^0.3.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.4.1"
  }
}
