{
  "name": "@claasahl/spotware-adapter",
  "version": "0.4.5",
  "description": "Protocol Buffers and Data Models for Spotware Open API 2.0",
  "main": "./build/index.js",
  "private": false,
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "prebuild": "shx rm -rf ./build",
    "build": "npm run import && npm run generate && tsc",
    "playground": "ts-node src/playground.ts",
    "preimport": "shx rm -rf ./assets/*.proto",
    "import": "shx mkdir ./spotware-messages && cd ./spotware-messages && isogit clone --url=https://github.com/spotware/Open-API-2.0-protobuf-messages --singleBranch",
    "postimport": "shx cp ./spotware-messages/*.proto ./assets && shx rm -rf ./spotware-messages",
    "pregenerate": "shx mkdir ./build",
    "generate": "npm run generate:js && npm run generate:ts && npm run generate:messages:map && npm run generate:payload-types && npm run generate:messages && npm run generate:messages:index",
    "generate:js": "pbjs --no-delimited --no-verify --no-create --no-convert --force-long --target static-module --wrap commonjs --out ./src/spotware-messages.js ./assets/*.proto",
    "postgenerate:js": "prettier --write ./src/spotware-messages.js && shx cp ./src/spotware-messages.js ./build/",
    "generate:ts": "pbts --out ./src/spotware-messages.d.ts ./src/spotware-messages.js",
    "postgenerate:ts": "prettier --write ./src/spotware-messages.d.ts && shx cp ./src/spotware-messages.d.ts ./build/",
    "generate:messages:map": "ts-node ./scripts/00-compile-map.ts > ./assets/message-map.json && prettier --write ./assets/message-map.json",
    "generate:messages": "ts-node ./scripts/messages.ts",
    "generate:payload-types": "mustache ./assets/message-map.json ./assets/mustachios/spotware-payload-types.mustache > ./src/spotware-payload-types.ts",
    "postgenerate:payload-types": "prettier --write ./src/spotware-payload-types.ts",
    "postgenerate:messages": "prettier --write ./src/messages/**",
    "generate:messages:index": "mustache ./assets/message-map.json ./assets/mustachios/messages/index.mustache > ./src/messages/index.ts",
    "postgenerate:messages:index": "prettier --write ./src/messages/index.ts",
    "generate:messages:handler": "mustache ./assets/message-map.json ./assets/mustachios/spotware-message-handler.mustache > ./src/spotware-message-handler.ts",
    "postgenerate:messages:handler": "prettier --write ./src/spotware-message-handler.ts"
  },
  "keywords": [
    "spotware",
    "ctrader",
    "typescript",
    "protobuf"
  ],
  "author": {
    "name": "Claas Ahlrichs",
    "email": "claasahl@gmail.com",
    "url": "https://github.com/claasahl"
  },
  "repository": "github:claasahl/spotware-adapter",
  "bugs": {
    "url": "https://github.com/claasahl/spotware-adapter/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/long": "^4.0.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.12.2",
    "@types/protocol-buffers-schema": "^3.3.0",
    "husky": "^1.3.1",
    "isomorphic-git": "^0.53.0",
    "jest": "^24.5.0",
    "mustache": "^3.0.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "protocol-buffers-schema": "^3.3.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.4.1"
  },
  "dependencies": {
    "@claasahl/spotware-protobuf": "^0.1.4",
    "long": "^4.0.0"
  }
}
