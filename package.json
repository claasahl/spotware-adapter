{
  "name": "@claasahl/spotware-connect-api",
  "version": "0.1.8",
  "description": "Protocol Buffers and Data Models for Spotware Open API 2.0",
  "main": "./build/index.js",
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "prebuild": "shx rm -rf ./build",
    "build": "npm run import && npm run generate && tsc",
    "playground": "ts-node src/playground.ts",
    "preimport": "shx rm -rf ./assets/*.proto",
    "import": "shx mkdir ./spotware-messages && cd ./spotware-messages && isogit clone --url=https://github.com/spotware/Open-API-2.0-protobuf-messages --singleBranch",
    "postimport": "shx cp ./spotware-messages/*.proto ./assets && shx rm -rf ./spotware-messages",
    "pregenerate": "shx mkdir ./build",
    "generate": "npm run generate:js && npm run generate:ts && npm run generate:event-emitter && npm run generate:client",
    "generate:js": "pbjs --target static-module --wrap commonjs --out ./src/spotware-messages.js ./assets/*.proto",
    "postgenerate:js": "prettier --write ./src/spotware-messages.js && shx cp ./src/spotware-messages.js ./build/",
    "generate:ts": "pbts --out ./src/spotware-messages.d.ts ./src/spotware-messages.js",
    "postgenerate:ts": "prettier --write ./src/spotware-messages.d.ts && shx cp ./src/spotware-messages.d.ts ./build/",
    "generate:event-emitter": "ts-node ./scripts/spotware-event-emitter.ts",
    "postgenerate:event-emitter": "prettier --write ./src/spotware-event-emitter.ts",
    "generate:client": "ts-node ./scripts/spotware-client.ts",
    "postgenerate:client": "prettier --write ./src/spotware-client.ts",
    "something": "ts-node ./scripts/compile-message-map.ts && prettier --write ./assets/message-map.json",
    "compile": "npm run 00-map && npm run 05-events && npm run 10-event-emitter && npm run compile-readme",
    "00-map": "ts-node ./scripts/00-compile-map.ts > ./assets/message-map.json && prettier --write ./assets/message-map.json",
    "05-events": "mustache ./assets/message-map.json ./assets/spotware-events.mustache > ./src/spotware-events.ts && prettier --write ./src/spotware-events.ts",
    "10-event-emitter": "ts-node ./scripts/10-compile-event-emitter.ts > ./src/spotware-event-emitter.ts && prettier --write ./src/spotware-event-emitter.ts",
    "compile-readme": "npm run readme-table-events && npm run readme-table-proto-events",
    "prereadme-table-events": "shx head -n 39 ./README.md > ./README.trimmed.md && shx mv -f ./README.trimmed.md ./README.md",
    "readme-table-events": "ts-node ./scripts/readme-table-events.ts >> ./README.md",
    "readme-table-proto-events": "ts-node ./scripts/readme-table-proto-events.ts >> ./README.md",
    "postreadme-table-proto-events": "prettier --write ./README.md",
    "generate:messages": "ts-node ./scripts/messages.ts",
    "postgenerate:messages": "prettier --write ./src/messages/**"
  },
  "keywords": [
    "spotware",
    "ctrader",
    "typescript",
    "ts",
    "api",
    "protobuf"
  ],
  "author": {
    "name": "Claas Ahlrichs",
    "email": "claasahl@gmail.com",
    "url": "https://github.com/claasahl"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.12.2",
    "@types/protocol-buffers-schema": "^3.3.0",
    "husky": "^1.3.1",
    "isomorphic-git": "^0.53.0",
    "jest": "^24.5.0",
    "mustache": "^3.0.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "protocol-buffers-schema": "^3.3.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.4.1"
  }
}
