{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build",
    "build": "npm run gen && tsc",
    "start": "ts-node -r dotenv/config src/server.ts",
    "gen": "npm run gen:graphql && npm run gen:protobuf:js && npm run gen:protobuf:ts",
    "gen:protobuf:js": "pbjs -t static-module -w commonjs -o ./src/generated/spotware.js node_modules/@claasahl/spotware-protobuf-messages/*.proto",
    "gen:protobuf:ts": "pbts -o ./src/generated/spotware.d.ts ./src/generated/spotware.js",
    "gen:graphql": "gql-gen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@claasahl/spotware-protobuf-messages": "claasahl/Open-API-2.0-protobuf-messages#fe95b342bba568d790c3a74a165d204bf33eeaee",
    "@types/aws-lambda": "^8.10.19",
    "@types/axios": "^0.14.0",
    "@types/node": "^11.9.5",
    "@types/ws": "^6.0.1",
    "dotenv": "^6.2.0",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-add": "^0.17.0",
    "graphql-codegen-typescript-common": "0.17.0",
    "graphql-codegen-typescript-resolvers": "^0.17.0",
    "graphql-codegen-typescript-server": "^0.17.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "graphql-tag": "^2.10.1",
    "graphql-yoga": "^1.17.4"
  }
}
